{
  "name": "augustium",
  "version": "0.1.0",
  "description": "The Augustium programming language compiler and virtual machine for blockchain smart contracts",
  "main": "index.js",
  "bin": {
    "augustium": "./bin/augustium",
    "augustc": "./bin/augustc",
    "august": "./bin/august"
  },
  "scripts": {
    "install": "node scripts/install.js",
    "postinstall": "node scripts/postinstall.js",
    "test": "node scripts/test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AugustMoreau/augustium.git"
  },
  "keywords": [
    "augustium",
    "blockchain",
    "smart-contracts",
    "compiler",
    "virtual-machine",
    "cryptocurrency",
    "web3",
    "defi",
    "solidity-alternative"
  ],
  "author": "August Moreau <august@augustium.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AugustMoreau/augustium/issues"
  },
  "homepage": "https://augustium.org",
  "engines": {
    "node": ">=14.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "dependencies": {
    "node-fetch": "^3.3.0",
    "tar": "^6.1.0",
    "which": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "bin/",
    "scripts/",
    "index.js",
    "README.md",
    "LICENSE"
  ],
  "preferGlobal": true,
  "publishConfig": {
    "access": "public"
  }
